{
    "Function Definition": {
        "prefix": ["function"],
        "body": ["function ${1:name}${2: with args} do\n\nend"]
    },
    "Object Definition": {
        "prefix": ["object"],
        "body": ["object ${1:name}${2: with args} do\n\nend"]
    },
    "Count Loop": {
        "prefix": ["count"],
        "body": ["count from ${1:start} as ${2:i} to ${3:_end}${4: step _step} do\n\nend"]
    },
    "While Loop": {
        "prefix": ["while"],
        "body": ["while ${1:statement} do\n\nend"]
    },
    "Try Statement": {
        "prefix": ["try"],
        "body": ["try do\n\nerror ${1:'ERROR-TAG'}${2: as _error} do\n\nend"]
    }
}