{
  "name": "ezr-language-support",
  "displayName": "ezr language support",
  "description": "Official VSCode extension for the ezr programming language!",
  "author": {"email": "uralstech@gmail.com", "name": "Udayshankar R", "url": "https://uralstech.in/"},
  "publisher": "Uralstech",
  "version": "1.4.0",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Themes"
  ],
  "repository": {"url": "https://github.com/Uralstech/ezr-language-support"},
  "icon": "graphics/ezrIcon (128x128).png",
  "contributes": {
    "languages": [{
      "id": "ezr",
      "aliases": ["ezr", "ezr"],
      "extensions": [".ezr"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "ezr",
      "scopeName": "source.ezr",
      "path": "./syntaxes/ezr.tmLanguage.json"
    }],
    "iconThemes": [
      {
        "id": "ezr",
        "label": "ezr",
        "path": "./fileicons/ezr-icon-theme.json"
      }
    ],
    "snippets": [
      {
        "language": "ezr",
        "path": "./snippets/ezr_snippets.json"
      }
    ]
  }
}
